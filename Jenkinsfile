pipeline {
  agent any
  
  stages {
    stage("build") {
      steps {
        echo "build application from Jenkins"
      }
    }
    
    stage("test") {
      steps {
        echo "test application from Jenkins"
      }
    }
    
    stage("deploy") {
      steps {
        echo "deploy application from Jenkins"
      }
    }
    
  }
}
